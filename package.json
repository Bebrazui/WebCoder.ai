
{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "main": "electron.cjs",
  "scripts": {
    "dev": "next dev --turbopack -p 9002",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "compile-java": "find java_apps/src -name \"*.java\" > java_apps/sources.txt && javac -d java_apps/build -cp \"java_apps/lib/*\" @java_apps/sources.txt && rm java_apps/sources.txt",
    "compile-go": "go build -o go_apps/my_go_app ./go_apps",
    "compile-rust": "cd rust_apps && cargo build --release && cd .. && cp rust_apps/target/release/rust_apps rust_apps/my_rust_app",
    "compile-csharp": "cd csharp_apps/my_csharp_app && dotnet publish -c Release -o ../build",
    "electron:dev": "electron .",
    "electron:build": "next build && electron-builder"
  },
  "dependencies": {
